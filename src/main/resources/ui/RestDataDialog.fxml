<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="350" minWidth="310" prefHeight="350.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.urkallinger.restclient.controller.RestDataDialogController">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<bottom>
		<ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<buttons>
				<Button text="OK" />
				<Button text="Cancel" />
			</buttons>
		</ButtonBar>
	</bottom>
	<center>
		<TreeTableView fx:id="treeTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<columns>
				<TreeTableColumn fx:id="colProject" style="-fx-font-size: 14px; -fx-font-weight: normal;" text="Project" />
				<TreeTableColumn fx:id="colName" style="-fx-font-size: 14px; -fx-font-weight: normal;" text="Configuration name" />
			</columns>
			<columnResizePolicy>
				<TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TreeTableView>
	</center>
</BorderPane>
